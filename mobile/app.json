        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#202024"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.outumegames.escalafacil"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "react-native-edge-to-edge",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c6d41128-f5d1-45a8-ba0d-08b61ebb2798"
